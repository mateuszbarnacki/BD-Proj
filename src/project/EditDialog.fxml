<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="project.EditDialogController">
    <center>
        <GridPane alignment="CENTER" hgap="10" vgap="15">
            <Label text="Nowy login: " textAlignment="CENTER" GridPane.rowIndex="0" GridPane.columnIndex="0" style="-fx-font-size: 15px;"/>
            <TextField fx:id="login" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Nowe hasło: " textAlignment="CENTER" GridPane.rowIndex="1" GridPane.columnIndex="0" style="-fx-font-size: 15px;"/>
            <PasswordField fx:id="password" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Powtórz hasło: " textAlignment="CENTER" GridPane.rowIndex="2" GridPane.columnIndex="0" style="-fx-font-size: 15px;"/>
            <PasswordField fx:id="secPassword" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        </GridPane>
    </center>
</BorderPane>
